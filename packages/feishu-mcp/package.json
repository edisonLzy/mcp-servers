  "author": "MCP Servers Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "catalog:",
    "axios": "^1.6.0",
    "commander": "^12.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "inquirer": "^10.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "engines": {
    "node": ">=18"
  }
