{
  "name": "feishu-mcp",
  "version": "0.1.0",
  "description": "MCP server for Feishu/Lark integration with Wiki and Document management",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "feishu-mcp": "dist/cli.js"
  },
  "scripts": {
    "test": "vitest",
    "inspector": "pnpx @modelcontextprotocol/inspector tsx src/index.ts",
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "cli": "tsx src/cli.ts",
    "start": "node dist/index.js",
    "cli:built": "node dist/cli.js",
    "login": "tsx src/cli.ts login",
    "logout": "tsx src/cli.ts logout",
    "whoami": "tsx src/cli.ts whoami"
  },
  "keywords": [
    "mcp",
    "feishu",
    "lark",
    "wiki",
    "documents",
    "collaboration"
