{
  "mcpServers": {
    "lark_open_doc_search": {
      "command": "npx",
      "args": [
        "-y",
        "@larksuiteoapi/lark-mcp",
        "recall-developer-documents"
      ]
    },
    "playwright": {
        "command": "npx",
        "args": [
          "@playwright/mcp@latest",
          "--extension"
        ]
    },
    "feishu": {
      "command": "codemons-mcp",
      "args": ["feishu-mcp"]
    }
  }
}